{
    "version": 2,
    "builds": [
      {
        "src": "src/server.ts",
        "use": "@vercel/node",
        "config": {
          "includeFiles": ["prisma/schema.prod.prisma", ".env"]
        }
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/src/server.ts"
      }
    ]
  }
  